{
  "manifest_version": 2,
  "name": "Tangerino remember",
  "version": "1.0",
  "description": "Extension to move me to tangerino pages on time.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "tangerinoIcon.png"
  },
  "permissions": [
    "alarms",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}


